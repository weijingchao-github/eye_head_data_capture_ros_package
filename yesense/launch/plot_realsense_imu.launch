<launch>

  <include file="$(find realsense2_camera)/launch/rs_camera.launch">
    <arg name="color_width" value="640" />
    <arg name="color_height" value="480" />
    <arg name="color_fps" value="60" />
    <arg name="depth_width" value="640" />
    <arg name="depth_height" value="480" />
    <arg name="depth_fps" value="60" />
    <arg name="enable_sync" value="true" />
    <arg name="align_depth" value="true" />
    <arg name="enable_gyro" value="true" />
    <arg name="enable_accel" value="true" />
    <arg name="unite_imu_method" value="linear_interpolation" />
  </include>

  <include file="$(find imu_complementary_filter)/launch/complementary_filter.launch" />
  <!-- <include file="$(find imu_filter_madgwick)/launch/imu_filter_madgwick.launch" /> -->

  <node pkg="yesense_imu" type="imu_plot.py" name="imu_plot" output="screen" />

</launch>
